<?xml version="1.0" encoding="UTF-8"?>
<manifest>

  <remote  name="github"
           fetch="https://github.com/" />

  <remote  name="gitlab"
           fetch="https://gitlab.com/" />

  <remote  name="pdd"
           fetch="https://github.com/PixelDust-Devices"
           revision="refs/heads/12" />

  <remote  name="pd12"
           fetch="https://github.com/PixelDust-next"
           revision="refs/heads/12" />

  <!-- Manifest -->
  <project path="manifest" name="manifest" remote="pd12" />

  <!-- Build -->
  <project path="build/make" name="android_build" remote="pd12" >
    <copyfile src="core/root.mk" dest="Makefile" />
    <linkfile src="CleanSpec.mk" dest="build/CleanSpec.mk" />
    <linkfile src="buildspec.mk.default" dest="build/buildspec.mk.default" />
    <linkfile src="core" dest="build/core" />
    <linkfile src="envsetup.sh" dest="build/envsetup.sh" />
    <linkfile src="target" dest="build/target" />
    <linkfile src="tools" dest="build/tools" />
  </project>
  <project path="build/soong" name="android_build_soong" remote="pd12" >
    <linkfile src="root.bp" dest="Android.bp" />
    <linkfile src="bootstrap.bash" dest="bootstrap.bash" />
  </project>
  <project path="libcore" name="android_libcore" remote="pd12" />

  <!-- External -->
  <!-- Frameworks -->
  <!-- Hardware -->
  <!-- Native -->
  <!-- Packages -->
  <project path="packages/modules/NeuralNetworks" name="android_packages_modules_NeuralNetworks" remote="pd12" />

  <!-- Recovery -->
  <project path="bootable/recovery" name="android_bootable_recovery" remote="pd12" />

  <!-- System -->

  <!-- Vendor -->
  <project path="vendor/pixeldust" name="android_vendor_pixeldust" remote="pd12" >
    <copyfile src="build.sh" dest="rom-build.sh" />
  </project>

</manifest>
